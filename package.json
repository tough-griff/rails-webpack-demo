{
  "name": "rails-webpack-demo",
  "version": "0.0.1",
  "description": "Starter configuration for a Rails project using webpack for JavaScript assets",
  "repository": "tough-griff/rails-webpack-demo",
  "author": "Griffin Yourick",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "npm run clean",
    "build": "webpack --env.production --config frontend/config/webpack.config.babel.js",
    "postbuild": "npm run clean",
    "check": "npm run lint && npm run test",
    "clean": "rimraf app/assets/{javascripts,stylesheets}",
    "lint": "eslint --ext .js,.jsx frontend/",
    "start": "npm run webpack-server",
    "test": "cross-env NODE_ENV=test mocha --opts .mocha frontend/test/**/*.spec.{js,jsx}",
    "test:cov": "nyc npm run test",
    "test:watch": "npm run test -- --watch --growl",
    "prewebpack-server": "npm run clean",
    "webpack-server": "babel-node frontend/config/webpackServer.js"
  },
  "engines": {
    "node": ">= 4",
    "npm": ">= 3"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-polyfill": "^6.16.0",
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "key-code": "0.0.1",
    "lodash": "^4.17.2",
    "pluralize": "^3.0.0",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-immutable": "^3.0.8",
    "redux-saga": "^0.14.0",
    "reselect": "^2.5.4",
    "todomvc-app-css": "^2.0.6"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-lodash": "^3.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-register": "^6.18.0",
    "case-sensitive-paths-webpack-plugin": "^1.1.4",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "chai-immutable": "^1.6.0",
    "cheerio": "^0.22.0",
    "cors": "^2.8.1",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.0",
    "dirty-chai": "^1.2.2",
    "enzyme": "^2.6.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "jsdom": "^9.8.3",
    "jsdom-global": "^2.1.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "moxios": "^0.3.0",
    "node-sass": "^4.0.0",
    "nyc": "^10.0.0",
    "react-addons-test-utils": "^15.4.1",
    "react-dev-utils": "^0.4.2",
    "redux-mock-store": "^1.2.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.0-rc.2",
    "webpack-bundle-analyzer": "^2.1.1",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  },
  "nyc": {
    "exclude": [
      "frontend/test/**/*"
    ],
    "report-dir": "coverage/js",
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "instrument": false,
    "sourceMap": false
  }
}
