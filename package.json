{
  "name": "rails-webpack-starter",
  "version": "0.0.1",
  "description": "Starter configuration for a Rails project using webpack for JavaScript assets",
  "repository": "tough-griff/rails-webpack-starter",
  "bugs": {
    "url": "https://github.com/tough-griff/rails-webpack-starter/issues"
  },
  "homepage": "https://github.com/tough-griff/rails-webpack-starter#readme",
  "author": "Griffin Yourick",
  "license": "MIT",
  "keywords": [
    "rails",
    "webpack"
  ],
  "scripts": {
    "build:webpack": "NODE_ENV=production webpack --config=webpack.config.prod.babel.js",
    "build": "npm run clean && npm run build",
    "check": "npm run lint && npm run test",
    "clean": "rimraf app/assets/javascripts/*.bundle.*",
    "lint": "eslint --ext=.js,.jsx js/ *.js",
    "start": "npm run webpack",
    "test": "NODE_ENV=test mocha --compilers=js:babel-register --recursive",
    "webpack": "babel-node webpack.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.3",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.5.2",
    "babel-runtime": "^6.5.0",
    "cors": "^2.7.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^3.16.1",
    "express": "^4.13.4",
    "lodash.merge": "^4.3.2",
    "mocha": "^2.4.5",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.2",
    "redbox-react": "^1.2.2",
    "webpack": "^1.12.13",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.7.1"
  }
}
