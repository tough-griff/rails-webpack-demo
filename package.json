{
  "name": "rails-webpack-demo",
  "version": "0.0.1",
  "description": "Starter configuration for a Rails project using webpack for JavaScript assets",
  "repository": "tough-griff/rails-webpack-demo",
  "bugs": {
    "url": "https://github.com/tough-griff/rails-webpack-demo/issues"
  },
  "homepage": "https://github.com/tough-griff/rails-webpack-demo#readme",
  "author": "Griffin Yourick",
  "license": "MIT",
  "keywords": [
    "rails",
    "webpack"
  ],
  "scripts": {
    "build": "NODE_ENV=production webpack --config=frontend/webpack.config.prod.babel.js",
    "postbuild": "rimraf app/assets/javascripts/*.style.bundle.*",
    "check": "npm run lint && npm run test",
    "clean": "rimraf app/assets/{javascripts,stylesheets}",
    "lint": "eslint --ext=.js,.jsx frontend/",
    "start": "npm run webpack-server",
    "test": "NODE_ENV=test mocha --compilers=js:babel-register --require=./frontend/test/support/jsdom --recursive frontend/test",
    "test:cov": "NODE_ENV=test babel-node $(npm bin)/babel-istanbul cover $(npm bin)/_mocha -- --require=./frontend/test/support/jsdom --recursive --reporter=dot frontend/test",
    "test:watch": "npm run test -- --watch --growl",
    "webpack-server": "babel-node frontend/webpackServer.js"
  },
  "engines": {
    "node": "5.x",
    "npm": "3.x"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.0",
    "camel-case": "^1.2.2",
    "classnames": "^2.2.5",
    "constant-mirror": "^1.1.1",
    "fetch-check-http-status": "^2.0.0",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.13.1",
    "react": "^15.1.0",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "todomvc-app-css": "^2.0.6"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-istanbul": "^0.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.9.0",
    "cors": "^2.7.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.11.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "expect.js": "^0.3.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch-mock": "^4.5.0",
    "jsdom": "^9.2.0",
    "mocha": "^2.5.3",
    "node-sass": "^3.7.0",
    "react-addons-test-utils": "^15.1.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-mock-store": "^1.0.4",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
