{
  "name": "rails-webpack-demo",
  "version": "0.0.1",
  "description": "Starter configuration for a Rails project using webpack for JavaScript assets",
  "repository": "tough-griff/rails-webpack-demo",
  "bugs": {
    "url": "https://github.com/tough-griff/rails-webpack-demo/issues"
  },
  "homepage": "https://github.com/tough-griff/rails-webpack-demo#readme",
  "author": "Griffin Yourick",
  "license": "MIT",
  "keywords": [
    "rails",
    "webpack"
  ],
  "scripts": {
    "build:webpack:dev": "NODE_ENV=development webpack --config=js/webpack.config.dev.babel.js",
    "build:webpack": "NODE_ENV=production webpack --config=js/webpack.config.prod.babel.js",
    "build": "npm run clean && npm run build:webpack && npm run clean:after-build",
    "check": "npm run lint && npm run test",
    "clean:after-build": "rimraf app/assets/javascripts/*.style.bundle.*",
    "clean": "rimraf app/assets/{javascripts,stylesheets}/*.bundle.*",
    "lint": "eslint --ext=.js,.jsx js/",
    "start": "npm run webpack-server",
    "test": "NODE_ENV=test mocha --compilers=js:babel-register --recursive js/test",
    "webpack-server": "npm run clean && babel-node js/webpackServer.js"
  },
  "engines": {
    "node": "5.x",
    "npm": "3.x"
  },
  "dependencies": {
    "babel-polyfill": "^6.6.1",
    "normalize.css": "^3.0.3",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.6.5",
    "babel-runtime": "^6.6.1",
    "cors": "^2.7.1",
    "css-loader": "^0.23.1",
    "eslint": "2.2.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.2.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "lodash.mapvalues": "^4.2.0",
    "lodash.merge": "^4.3.2",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.3",
    "redbox-react": "^1.2.2",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.9.1"
  }
}
